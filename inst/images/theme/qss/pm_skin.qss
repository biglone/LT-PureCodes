/* QSpinBox */
QSpinBox, QDoubleSpinBox {
	padding-top: 2px;
	padding-left: 2px;
	padding-right: 2px;
	padding-bottom: 2px;
	border-image: url(:/theme/spinbox/spinbox_normal.png);
	border-top: 1px;
	border-left: 1px;
	border-right: 1px;
	border-bottom: 1px;
	color: #333333;
}
QSpinBox:hover:!disabled, QDoubleSpinBox:hover:!disabled {
	border-image: url(:/theme/spinbox/spinbox_hover.png);
}
QSpinBox:focus:!disabled, QDoubleSpinBox:focus:!disabled {
	border-image: url(:/theme/spinbox/spinbox_pressed.png);
}
QSpinBox:disabled, QDoubleSpinBox:disabled {
	border-image: url(:/theme/spinbox/spinbox_disabled.png);
	color: #666666;
}
QSpinBox::up-button, QDoubleSpinBox::up-button {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 21px;
	border-image: url(:/theme/spinbox/spinbox_upbutton_normal.png);
}
QSpinBox::up-button:hover, QDoubleSpinBox::up-button:hover {
