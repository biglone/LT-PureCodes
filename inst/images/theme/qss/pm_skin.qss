

/* QPushButton */
QPushButton{
		padding-right: 9px;
		padding-left: 9px;
		padding-top: 2px;
		padding-bottom: 2px;
		border-image: url(:/theme/pushbutton/pushbutton_normal.png);
		border-top: 0px;
		border-left: 0px;
		border-right: 0px;
		border-bottom: 0px;
		color: #ffffff;
}
QPushButton:hover:!pressed{
		border-image: url(:/theme/pushbutton/pushbutton_hover.png);
}
QPushButton:hover:pressed{
		border-image: url(:/theme/pushbutton/pushbutton_pressed.png);
}
QPushButton:disabled{
		color: #666666;
		border-image: url(:/theme/pushbutton/pushbutton_disabled.png);
}
QPushButton:focus{
		padding: -1px;
}

/* QLineEdit */
QLineEdit {
	padding-right: 2px;
	padding-left: 2px;
	padding-top: 2px;
	padding-bottom: 2px;
	border-image: url(:/theme/lineedit/lineedit_normal.png);
	border-top: 2px;
	border-left: 2px;
	border-right: 2px;
	border-bottom: 2px;
	color: #333333;
}
QLineEdit:!read-only:!disabled:hover {
	border-image: url(:/theme/lineedit/lineedit_hover.png);
}
QLineEdit:!read-only:!disabled:focus {
	border-image: url(:/theme/lineedit/lineedit_pressed.png);
}
QLineEdit:disabled {
	border-image: url(:/theme/lineedit/lineedit_disabled.png);
	color: #666666;
}
QLineEdit:read-only {
	border-image: url(:/theme/lineedit/lineedit_readonly.png);
	color: #444444;
}

/* QTextEdit */
QTextEdit {
	padding-right: 2px;
	padding-left: 2px;
	padding-top: 2px;
	padding-bottom: 2px;
	border-image: url(:/theme/lineedit/lineedit_normal.png);
	border-top: 2px;
	border-left: 2px;
	border-right: 2px;
	border-bottom: 2px;
	color: #333333;
}
QTextEdit[readOnly=\"false\"]:!disabled:hover {
	border-image: url(:/theme/lineedit/lineedit_hover.png);
}
QTextEdit[readOnly=\"false\"]:!disabled:focus {
	border-image: url(:/theme/lineedit/lineedit_pressed.png);
}
QTextEdit:disabled {
	border-image: url(:/theme/lineedit/lineedit_disabled.png);
	color: #666666;
}
QTextEdit[readOnly=\"true\"] {
	border-image: url(:/theme/lineedit/lineedit_readonly.png);
	color: #444444;
}

/* QCheckBox */
QCheckBox {
	spacing: 5px;
	color: #333333;
}
QCheckBox::indicator {
	width: 14px;
	height: 14px;
}
QCheckBox::indicator:unchecked {
	image: url(:/theme/checkbox/checkbox_unchecked_normal.png);
}
QCheckBox::indicator:unchecked:hover {
	image: url(:/theme/checkbox/checkbox_unchecked_hover.png);
}
QCheckBox::indicator:unchecked:pressed {
	image: url(:/theme/checkbox/checkbox_unchecked_pressed.png);
}
QCheckBox::indicator:unchecked:disabled {
	image: url(:/theme/checkbox/checkbox_unchecked_disabled.png);
}
QCheckBox::indicator:checked {
	image: url(:/theme/checkbox/checkbox_checked_normal.png);
}
QCheckBox::indicator:checked:hover {
	image: url(:/theme/checkbox/checkbox_checked_hover.png);
}
QCheckBox::indicator:checked:pressed {
	image: url(:/theme/checkbox/checkbox_checked_pressed.png);
}
QCheckBox::indicator:checked:disabled {
	image: url(:/theme/checkbox/checkbox_checked_disabled.png);
}
QCheckBox::indicator:indeterminate {
	image: url(:/theme/checkbox/checkbox_indeterminate_normal.png);
}
QCheckBox::indicator:indeterminate:hover {
	image: url(:/theme/checkbox/checkbox_indeterminate_hover.png);
}
QCheckBox::indicator:indeterminate:pressed {
	image: url(:/theme/checkbox/checkbox_indeterminate_pressed.png);
}
QCheckBox::indicator:indeterminate:disabled {
	image: url(:/theme/checkbox/checkbox_indeterminate_disabled.png);
}
QCheckBox:disabled {
	color: #666666;
}

