}
QSpinBox::up-button:disabled, QDoubleSpinBox::up-button:disabled {
	border-image: url(:/theme/spinbox/spinbox_upbutton_disabled.png);
}
QSpinBox::up-arrow, QDoubleSpinBox::up-arrow {
	image: none;
}
QSpinBox::down-button, QDoubleSpinBox::down-button {
	subcontrol-origin: padding;
	subcontrol-position: bottom right;
	width: 21px;
	border-image: url(:/theme/spinbox/spinbox_downbutton_normal.png);
}
QSpinBox::down-button:hover, QDoubleSpinBox::down-button:hover {
	border-image: url(:/theme/spinbox/spinbox_downbutton_hover.png);
}
QSpinBox::down-button:pressed, QDoubleSpinBox::down-button:pressed {
	border-image: url(:/theme/spinbox/spinbox_downbutton_pressed.png);
}
QSpinBox::down-button:disabled, QDoubleSpinBox::down-button:disabled {
	border-image: url(:/theme/spinbox/spinbox_downbutton_disabled.png);
}
QSpinBox::down-arrow, QDoubleSpinBox::down-arrow {
	image: none;
}
